
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<br />

<a asp-action="Create">Create Employee</a>

<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Salary</td>
    </tr>

    @foreach(var x in Model)
    {
        <tr>
            <td>@x.Id</td>
            <td>@x.Name</td>
            <td>@x.salary</td>
        </tr>
    }
</table>
